{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="card bg-light mb-3" >
            <div class="card-body">
                <h5 class="card-title"></h5>
        <div class="row" style="margin-top: +1%">

            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 pr-xl-0 pr-lg-0 pr-md-0  m-b-30" >


                <div class="product-slider">
                    <div class="card-body">
                        <img src="{{ asset('images/'~aa.image)}} " >
                        {{ form_start(form) }}
                        {{form_widget(form.rating)}}
                        <input type="submit" value="Rate"class="genric-btn primary circle" />

                        {{ form_end(form) }}
                    </div>


                </div>

            </div>

            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 pl-xl-0 pl-lg-0 pl-md-0 border-left m-b-30" >

                <div class="product-details" style="height: 455px">
                    <div class="border-bottom pb-3 mb-3">
                        <h1>Club Info</h1></div>
                    <h1>Id Club : {{ aa.getIdclub }}</h1>
                    <h1>Nom Club : {{ aa.getNomclub() }}</h1>
                    <h1>Domaine :{{ aa.getDomaine() }}</h1>




                    <ul>
                        <li>
                            <a href="{{ path('club_show_image') }}">Back to the list</a>
                        </li>




                    </ul>

                </div>

            </div>







        </div>
    </div>
        </div></div>
    {% endblock  %}
